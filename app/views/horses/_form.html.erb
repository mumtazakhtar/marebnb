<%= simple_form_for @horse do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :breed %>
  <%= f.input :price %>
  <%= f.input :location %>
  <%= f.input :description %>
  <%= f.input :user_id %>
  <%= f.input :photo, as: :file %>
  <%= f.association :user :value current_user %>
  <%= f.submit %>
<% end %>
